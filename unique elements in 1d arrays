#include<iostream>
using namespace std;
int main()
{
	int a[10],b[10],c=0;
	cout<<"enter first set of array :";
	for(int i=0;i<10;i++)
	{
		cin>>a[i];
	}
	cout<<"enter second set of array :";
	for(int i=0;i<10;i++)
	{
		cin>>b[i];
	}
	for(int j=0;j<10;j++)
	{
	for(int i=0;i<10;i++)
	{
		if(a[j]==b[i] && c==0)
		{
		cout<<a[j];
		c=1;
	}c=0;
	}
}
	return 0;
}
