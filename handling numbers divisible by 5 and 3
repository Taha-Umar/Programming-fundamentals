#include<iostream>
using namespace std;
int main()
{
	int i=1;
	while (i < 101)

	{


		cout <<  i ;
		if   (i % 5 == 0 && i % 3 == 0)
		{
			cout << " FizzBuzz ";
		}
		else if (i % 3 == 0)
		{
			cout << " Fizz ";

		}

		else if (i % 5 == 0)
		{
			cout << " Buzz ";
		}
		
		i = i + 1;
	}
	system("pause");
	return  0;
}
